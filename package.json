{
  "name": "angular2-translator",
  "version": "1.1.1",
  "description": "A translate-service, translate-pipe and translate-component for angular2",
  "repository": {
    "type": "git",
    "url": "git://gitlab.w00tserver.org/tflori/angular2-translator.git"
  },
  "scripts": {
    "test": "typings install && tsc && karma start",
    "tslint": "tslint -c tslint.json --exclude=\"**/*.d.ts\" --exclude=\"node_modules/**/*.ts\" \"**/*.ts\"",
    "jslint": "jslint --nomen --indent 2 -- bundle.js karma.conf.js karma-test-shim.js",
    "test-watch": "typings install && tsc && karma start --no-single-run --auto-watch",
    "prepublish": "typings install && tsc && node bundle.js"
  },
  "keywords": [
    "angular2",
    "translate",
    "i18n"
  ],
  "author": "Thomas Flori",
  "license": "MIT",
  "bugs": {
    "url": "http://gitlab.w00tserver.org/tflori/angular2-translator/issues"
  },
  "main": "./angular2-translator.js",
  "typescript": {
    "main": "./angular2-translator.ts"
  },
  "typings": "./angular2-translator.d.ts",
  "homepage": "http://gitlab.w00tserver.org/tflori/angular2-translator",
  "peerDependencies": {
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/http": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "@angular/router": "3.0.0-beta.2",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "0.6.12"
  },
  "devDependencies": {
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/http": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "@angular/router": "3.0.0-beta.2",
    "es6-shim": "0.35.1",
    "jasmine-core": "2.4.1",
    "jslint": "^0.10.3",
    "karma": "1.1.2",
    "karma-chrome-launcher": "1.0.1",
    "karma-coverage": "1.1.1",
    "karma-firefox-launcher": "1.0.0",
    "karma-jasmine": "1.0.2",
    "karma-spec-reporter": "0.0.26",
    "reflect-metadata": "0.1.3",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "0.19.27",
    "systemjs-builder": "0.15.6",
    "tslint": "^3.14.0",
    "typings": "1.0.4",
    "zone.js": "0.6.12"
  }
}
